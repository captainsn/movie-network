#accountsubmit
  font-family: "Roboto", sans-serif;
  text-transform: uppercase;
  outline: 0;
  background: #379E51;
  width: 100%;
  border: 0;
  padding: 15px;
  color: #FFFFFF;
  font-size: 14px;
  -webkit-transition: all 0.3 ease;
  transition: all 0.3 ease;
  cursor: pointer

.className
  float left
  margin-right: 20px; 
  margin-bottom: 0.5em
  height 300px
  width 200px


.jumbotron {
  padding-top: 1rem;
  padding-bottom: 6rem;
  margin-bottom: 0;
  background-color: #fff;
  text-align center
}

.jumbotron p:last-child {
  margin-bottom: 0;
  
}

.jumbotron-heading {
  font-weight: 300;
}

.jumbotron-heading h1 {
  font-family "Lato"
  opacity 0.7
  padding-bottom 25px
}

.p span {
  font-family "Lato"
}

.jumbotron .container {
  max-width: 40rem;
}

.p-link a {
  padding-bottom 1rem 
}

.album {
  min-height: 50rem; /* Can be removed; just added for demo purposes */
  padding-top: 3rem;
  padding-bottom: 10rem;
  padding-left 80px
  padding-right 80px
  background-color: #f7f7f7;
}



.card {
  float: left;
  width: 33.333%;
  padding: .75rem;
  margin-bottom: 2rem;
  border: 0;
}

.card > img {
  margin-bottom: .75rem;
}

.card-text {
  font-size: 85%;
}

footer {
  padding-top: 3rem;
  padding-bottom: 3rem;
}

footer p {
  margin-bottom: .25rem;
}